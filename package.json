{
  "name": "petni",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "type-check": "tsc",
    "start:watch": "(node-sass -w --output-style compressed Components -o Components) & (node-sass -w --output-style compressed styles -o styles)",
    "watch:component": "node-sass -w --output-style compressed Components -o Components",
    "watch:view": "node-sass -w --output-style compressed styles -o styles",
    "compile": "node-sass --output-style compressed Components -o Components && node-sass --output-style compressed styles -o styles"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.0",
    "classnames": "^2.3.1",
    "next": "12.1.0",
    "node-sass": "^7.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.6",
    "sass": "^1.49.9"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "autoprefixer": "^10.4.2",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "postcss-loader": "^6.2.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.5.1",
    "typescript": "4.6.2"
  },
  "pre-commit": [
    "lint",
    "type-check"
  ]
}